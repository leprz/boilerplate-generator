{
    "name": "leprz/php-code-generator",
    "description": "This package helps you generate any kind of php boilerplate - easily!",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "symfony/filesystem": "^5.2@dev",
        "nette/php-generator": "^3.4@dev",
        "nikic/php-parser": "^4.9@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4@dev",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "vimeo/psalm": "^4.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Przemek Łęczycki",
            "email": "leczycki.przemyslaw@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leprz\\Generator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
