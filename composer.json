{
    "name": "leprz/boilerplate-generator",
    "description": "This package helps you generate any kind of file boilerplate including PHP classes",
    "keywords": [
        "generator",
        "code generator",
        "scaffold",
        "boilerplate file"
    ],
    "type": "package",
    "require": {
        "php": ">=7.4",
        "symfony/filesystem": "^5.0",
        "nette/php-generator": "^3.0",
        "nikic/php-parser": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.0",
        "vimeo/psalm": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Przemek Łęczycki"
        }
    ],
    "autoload": {
        "psr-4": {
            "Leprz\\Boilerplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leprz\\Boilerplate\\Tests\\": "tests/",
            "Leprz\\Boilerplate\\Output\\": "tests/output/"
        }
    },
    "minimum-stability": "stable"
}
